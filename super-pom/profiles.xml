<?xml version="1.0" encoding="utf-8"?>
<profilesXml xmlns="http://maven.apache.org/PROFILES/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://maven.apache.org/PROFILES/1.0.0 http://maven.apache.org/xsd/profiles-1.0.0.xsd">
    <profiles>
        <profile>
            <id>default</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <robocode.version>1.9.0.0</robocode.version> <!-- Any string, but no spaces (use '-' instead of space) -->
                <robocode.dotnet.version>1.9.0.0</robocode.dotnet.version> <!-- Only X.X.X.X format -->
            </properties>
        </profile>
    </profiles>
</profilesXml>
